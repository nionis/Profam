{
  "name": "profam",
  "version": "1.0.97",
  "description": "Profanity and Spam Tool, supporting multiple languages and modes.",
  "keywords": [
    "profanity",
    "badwords",
    "spam",
    "troll",
    "filter",
    "sensor"
  ],
  "person": {
    "name": "Steven Noni",
    "email": "staurosnonis@gmail.com",
    "url": "https://twitter.com/StevenNonis"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nionis/profam.git"
  },
  "scripts": {
    "build": "babel source --out-file distribution/profam.js",
    "buildw": "babel -w source --out-file distribution/profam.js",
